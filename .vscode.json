{
    "go.formatTool": "gofumports",
    "go.useLanguageServer": true,
    "go.toolsManagement.autoUpdate": true,
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--enable=vet",
        "--enable=golint",
    ],
    "go.coverMode": "atomic",
    "go.coverOnSingleTest": true,
    "go.coverOnSingleTestFile": true,
    "go.testTimeout": "25s",
    "go.testFlags": [
        "-v",
        "-race",
        "-count=1",
        "-shuffle=on"
    ],
    "go.vetFlags": [
        "-all"
    ],
    "go.enableCodeLens": {
        "references": false,
        "runtest": true
    },
    "go.coverageDecorator": {
        "type": "gutter",
        "coveredHighlightColor": "rgba(64,128,128,0.5)",
        "uncoveredHighlightColor": "rgba(128,64,64,0.25)",
        "coveredGutterStyle": "blockblue",
        "uncoveredGutterStyle": "slashyellow"
    },
    "go.editorContextMenuCommands": {
        "toggleTestFile": false,
        "addTags": false,
        "removeTags": false,
        "testAtCursor": false,
        "testFile": false,
        "testPackage": false,
        "generateTestForFunction": false,
        "generateTestForFile": false,
        "generateTestForPackage": false,
        "addImport": false,
        "testCoverage": false,
        "playground": false,
        "debugTestAtCursor": false
    },
    "git.autoRepositoryDetection": false,
    "git.enableCommitSigning": true,
    "bookmarks.showCommandsInContextMenu": false,
    "breadcrumbs.icons": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "docker.showStartPage": false,
    "editor.insertSpaces": false,
    "editor.lightbulb.enabled": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.minimap.enabled": false,
    "editor.suggestSelection": "first",
    "editor.maxTokenizationLineLength": 1000,
    "editor.scrollBeyondLastLine": false,
    "editor.find.addExtraSpaceOnTop": false,
    "editor.snippetSuggestions": "none",
    "extensions.ignoreRecommendations": true,
    "files.autoSaveDelay": 1200,
    "files.associations": {
        "*.tmpl": "html",
        "*.svelte": "html"
    },
    "outline.icons": false,
    "outline.problems.badges": false,
    "outline.problems.colors": false,
    "outline.problems.enabled": false,
    "search.maxResults": 1000,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.shell.osx": "zsh",
    "window.autoDetectHighContrast": false,
    "window.zoomLevel": 2,
    "workbench.colorTheme": "Monokai",
    "workbench.welcomePage.preferReducedMotion": true,
    "workbench.activityBar.visible": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.tips.enabled": false,
    "diffEditor.renderSideBySide": false,
}
